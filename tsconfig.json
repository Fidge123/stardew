{
  "compilerOptions": {
    "target": "ES2017",
    "module": "commonjs",
    "outFile": "transform.js",
    "removeComments": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true
  },
  "include": [
    "transform.ts"
  ]
}
